
public class Car {
	//PROPERTIES
	public double weight;
	public double size;
	
	//METHODS
	public double calculateWeightOnMoon() {
		double weightOnMoon = weight * 3;
		return weightOnMoon;
	}
	
	public void setWeight(double number) {
		weight = number;
	}
	
	public double getWeight() {
		return weight;
	}
}

public class Example {
	public static void main(String args[]) {
		Car LeesCar;
		LeesCar = new Car();
		LeesCar.setWeight(10.5);
		System.out.println(LeesCar.getWeight());
		
		Car LukesCar;
		LukesCar = new Car();
		LukesCar.weight = 9.5;
		
		double LukesCarsWeightOnMoon = LukesCar.calculateWeightOnMoon();
		
		int[] hotelRoomsOnFloor1;
		hotelRoomsOnFloor1[1]=
	}
}
