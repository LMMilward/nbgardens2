
public class Example {
	public static void main(String[] args) {
		
		
		
		int total = 0; 
		int[] numList = new int[100];
		numList[100] = 0; 
		
		for (int counter = 0; counter < numList.length; counter++) {
			numList[counter] = counter;
			
		}
		for (int counter = 0; counter < numList.length; counter++)
		{
			total = total + counter;				
			
		}
			System.out.println("The Array total is " + total);	
		
		
		
		for (int counter = 100; counter <= numList.length; counter --)	
		{
			
			System.out.println("Array number is" + counter);
			
		}
			
			
			
	}
}
